	<append xpath="/items/item[@name='gunJunkTurretSchematic']/property[@name='Unlocks']/@value">ammoSteelJunkTurret,ammoBundleSteelJunkTurret</append>
	<append xpath="/items/item[@name='gunJunkTurret']/property[@class='Action0']/property[@name='Magazine_items']/@value">,ammoSteelJunkTurret</append>
	<set xpath="/items/item[@name='gunJunkTurret']/property[@name='RepairTools']/@value">resourceJunkTurretRepairKit</set>
	<set xpath="/items/item[@name='gunJunkTurret']/effect_group[@name='gunJunkTurret']/passive_effect[@name='MagazineSize' and @operation='base_set']/@value">60</set>
