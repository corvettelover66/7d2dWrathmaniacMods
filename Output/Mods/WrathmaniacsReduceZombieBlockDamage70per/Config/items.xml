<WrathmaniacsReduceZombieBlockDamage>
	<append xpath="items/item[contains(@name, 'HandZombie') or contains(@name, 'HandAnimal')]">
		<effect_group>
			<passive_effect name="BlockDamage"  operation="perc_add"  value="-.7"/>
		</effect_group>
	</append>
	<append xpath="items/item[contains(@name, 'Vomit')]">
		<effect_group>
			<passive_effect name="DamageModifier"  operation="perc_add"  value="-.7" tags="wood,metal,stone"/>
		</effect_group>
	</append>
</WrathmaniacsReduceZombieBlockDamage>
